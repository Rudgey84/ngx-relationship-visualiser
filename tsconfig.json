{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "ESNext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "ES2015",
    "typeRoots": ["node_modules/@types"],
    "lib": ["ES2018", "dom"],
    "paths": {
      "visualiser-graph": ["dist/visualiser-graph"],
      "visualiser-graph/*": ["dist/visualiser-graph/*"]
    },
  },
  "angularCompilerOptions": {
    "strictTemplates": true
  }
}